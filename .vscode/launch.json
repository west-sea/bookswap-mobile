  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      //   "program": "${workspaceFolder}\\node_modules\\expo\\AppEntry.js"
      "runtimeExecutable": "expo",
      "runtimeArgs": ["start"]
    },
    {
      "type": "node",
      "request": "launch",
      "console": "integratedTerminal",
      "name": "Dev Launch Program",
      "skipFiles": ["<node_internals>/**"],
      //   "program": "${workspaceFolder}\\node_modules\\expo\\AppEntry.js"
      "runtimeExecutable": "expo",
      "runtimeArgs": ["start", "--dev-client", "-p", "8812"]
    }
  ]
}
